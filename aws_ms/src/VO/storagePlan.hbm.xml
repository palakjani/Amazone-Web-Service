<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="VO.StoragePlanVo" table="storage">

		<id name="storageId" type="int" column="id">
			<generator class="increment" />
		</id>
		
		<property name="planName">
			<column name="planname" />
		</property>
		
		<property name="dataSize">
			<column name="dataSize" />
		</property>
		
		<property name="price">
			<column name="price" />
		</property>
		
		
		<property name="planDescription">
			<column name="plandescription" />
		</property>
		
		
	</class>
</hibernate-mapping>